	</servlet>
	<servlet-mapping>
		<servlet-name>login</servlet-name>
		<url-pattern>/login</url-pattern>
	</servlet-mapping>
  
  <listener>
    <listener-class>ensias.teams.config.InitialisationDaoFactory</listener-class>
