  	</servlet>
  	<servlet-mapping>
		 <servlet-name>usersDisplay</servlet-name>
		 <url-pattern>/usersDisplay</url-pattern>
	</servlet-mapping>
  
  
  	<servlet>
		<servlet-name>inscription</servlet-name>
		<servlet-class>ensias.teams.servlets.Singin</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>inscription</servlet-name>
		<url-pattern>/inscription</url-pattern>
	</servlet-mapping>
  
 	<servlet>
		<servlet-name>login</servlet-name>
